         MACRO
&N       PDPPRLG &CINDEX=,&FRAME=,&BASER=,&ENTRY=
         AIF     ('&ENTRY' EQ 'NO').NENT
*
         ENTRY   &N
.NENT    ANOP
&N       EQU   *
         USING *,&BASER
         SAVE  (14,12),,&N
         LR    &BASER,15
         L     15,76(13)
         ST    13,4(15)
         ST    15,8(13)
         LR    13,15
         A     15,=F'&FRAME'
         ST    15,76(13)
         MEND
